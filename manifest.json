{
  "manifest_version": 3,
  "name": "NoiTu.Pro - Internal Cheat",
  "version": "2.0.7",
  "description": "Extension tự động hỗ trợ chơi game nối từ tại NoiTu.Pro. Github: github.com/htch9999",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://github.com/*"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["config.js", "content.js"],
      "run_at": "document_end"
    }
  ],

  "action": {
    "default_popup": "popup.html",
    "default_title": "NoiTu.Pro - Internal Cheat"
  }
}